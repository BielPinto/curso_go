type  Category  {
 id: ID!
 name: String!
 description: String
 courses: [Course!]!
}

type Course {
   id: ID!
   name: String!
   description: Stringcategory: Category!
}

input NewCategory {
  name: String!
  description: String
}

input NewCourse {
  name: String!
  description: String
  categoryId: ID!
}

type Query {
  categories: [Category!]!
  courses: [Course!]!
}

type Mutation {
  creaCategory(input: NewCategory!):Category!
  creaCourse(input: NewCourse!):Course!
}